<style-scheme id="one-dark" _name="One Dark" version="1.0">
  <author>draKKar1969</author>
  <_description>Atom One Dark syntax scheme</_description>

  <metadata>
    <property name="variant">dark</property>
    <property name="light-variant">one</property>
  </metadata>

  <!-- Named Colors -->
  <color name="black_1"                             value="#282C34"/>
  <color name="blue_1"                              value="#61AFEF"/>
  <color name="blue_2"                              value="#528BFF"/>
  <color name="alpha_blue_1"                        value="#3584e433"/>
  <color name="gray_1"                              value="#5C6370"/>
  <color name="alpha_gray_1"                        value="#FFFFFF08"/>
  <color name="alpha_gray_4"                        value="#282C3480"/>
  <color name="green_1"                             value="#98C379"/>
  <color name="orange_1"                            value="#D19A66"/>
  <color name="orange_2"                            value="#E06C75"/>
  <color name="purple_1"                            value="#C678DD"/>
  <color name="white_1"                             value="#FFFFFF"/>

  <!-- SourceView Styles -->
  <style name="background-pattern"                  background="#FFFFFF08"/>
  <style name="bracket-match"                       background="#F6F5F420"/>
  <style name="bracket-mismatch"                    background="#F6F5F420" underline="error"/>
  <style name="current-line"                        background="#3584E410"/>
  <style name="current-line-number"                 foreground="#F6F5F4" bold="false" background="00000000"/>
  <style name="cursor"                              foreground="#528BFF"/>
  <style name="draw-spaces"                         foreground="#F6F5F420"/>
  <style name="line-numbers"                        foreground="#576074" background="00000000"/>
  <style name="line-numbers-border"                 background="#00000000"/>
  <style name="right-margin"                        foreground="F6F5F420" background="00000000"/>
  <style name="search-match"                        background="#3584E430"/>
  <style name="selection"                           background="#3584E430" foreground="#F6F5F4"/>
  <style name="text"                                foreground="#F6F5F4"/>

  <!-- Default Styles -->
  <style name="def:base-n-integer"                  foreground="orange_1"/>
  <style name="def:boolean"                         foreground="orange_1"/>
  <style name="def:builtin"                         foreground="orange_1"/>
  <style name="def:character"                       foreground="orange_1"/>
  <style name="def:comment"                         foreground="gray_1" italic="true"/>
  <style name="def:complex"                         foreground="orange_1"/>
  <style name="def:constant"                        foreground="orange_1"/>
  <style name="def:decimal"                         foreground="orange_1"/>
  <style name="def:deletion"                        strikethrough="true"/>
  <style name="def:doc-comment"                     foreground="gray_1" italic="true"/>
  <style name="def:doc-comment-element"             foreground="gray_1" italic="true"/>
  <style name="def:emphasis"                        italic="true"/>
  <style name="def:error"                           foreground="#ffffff" background="#f2777a"/>
  <style name="def:floating-point"                  foreground="orange_1"/>
  <style name="def:function"                        foreground="blue_1"/>
  <style name="def:heading"                         foreground="orange_2" bold="true"/>
  <style name="def:identifier"                      foreground="orange_2"/>
  <style name="def:keyword"                         foreground="purple_1"/>
  <style name="def:link-destination"                foreground="purple_1"/>
  <style name="def:link-text"                       foreground="blue_1"/>
  <style name="def:list-marker"                     bold="true"/>
  <style name="def:note"                            foreground="gray_1" italic="true"/>
  <style name="def:number"                          foreground="orange_1"/>
  <style name="def:operator"                        foreground="purple_1"/>
  <style name="def:preprocessor"                    foreground="orange_1"/>
  <style name="def:reserved"                        foreground="purple_1"/>
  <style name="def:shebang"                         foreground="gray_1" italic="true"/>
  <style name="def:special-char"                    foreground="orange_1"/>
  <style name="def:special-constant"                foreground="orange_1"/>
  <style name="def:statement"                       foreground="purple_1"/>
  <style name="def:string"                          foreground="green_1"/>
  <style name="def:strong-emphasis"                 bold="true"/>
  <style name="def:type"                            foreground="orange_2"/>
  <style name="def:underlined"                      underline="single"/>

  <!-- Other Styles -->
  <style name="css:at-rules"                        foreground="purple_1"/>
  <style name="css:color"                           foreground="white_1"/>
  <style name="css:keyword"                         foreground="orange_2"/>
  <style name="html:dtd"                            foreground="green_1"/>
  <style name="html:tag"                            foreground="orange_2"/>
  <style name="js:function"                         foreground="blue_1"/>
  <style name="perl:builtin"                        foreground="blue_1"/>
  <style name="perl:include-statement"              foreground="purple_1"/>
  <style name="perl:special-variable"               foreground="orange_1"/>
  <style name="perl:variable"                       foreground="orange_2"/>
  <style name="php:string"                          foreground="green_1"/>
  <style name="python:builtin-constant"             foreground="purple_1"/>
  <style name="python:builtin-function"             foreground="blue_1"/>
  <style name="python:module-handler"               foreground="purple_1"/>
  <style name="python:special-variable"             foreground="purple_1"/>
  <style name="ruby:attribute-definition"           foreground="purple_1"/>
  <style name="ruby:builtin"                        foreground="orange_2"/>
  <style name="ruby:class-variable"                 foreground="orange_2"/>
  <style name="ruby:constant"                       foreground="orange_2"/>
  <style name="ruby:global-variable"                foreground="blue_1"/>
  <style name="ruby:instance-variable"              foreground="orange_2"/>
  <style name="ruby:module-handler"                 foreground="purple_1"/>
  <style name="ruby:predefined-variable"            foreground="orange_1"/>
  <style name="ruby:regex"                          foreground="orange_2"/>
  <style name="ruby:special-variable"               foreground="purple_1"/>
  <style name="ruby:symbol"                         foreground="white_1"/>
  <style name="rubyonrails:attribute-definition"    foreground="purple_1"/>
  <style name="rubyonrails:block-parameter"         foreground="orange_1"/>
  <style name="rubyonrails:builtin"                 foreground="orange_2"/>
  <style name="rubyonrails:class-inherit"           foreground="green_1"/>
  <style name="rubyonrails:class-name"              foreground="orange_2"/>
  <style name="rubyonrails:class-variable"          foreground="orange_2"/>
  <style name="rubyonrails:complex-interpolation"   foreground="orange_1"/>
  <style name="rubyonrails:constant"                foreground="orange_2"/>
  <style name="rubyonrails:global-variable"         foreground="blue_1"/>
  <style name="rubyonrails:instance-variable"       foreground="orange_2"/>
  <style name="rubyonrails:module-handler"          foreground="purple_1"/>
  <style name="rubyonrails:module-name"             foreground="orange_2"/>
  <style name="rubyonrails:predefined-variable"     foreground="orange_1"/>
  <style name="rubyonrails:rails"                   foreground="orange_2"/>
  <style name="rubyonrails:regex"                   foreground="orange_2"/>
  <style name="rubyonrails:simple-interpolation"    foreground="orange_1"/>
  <style name="rubyonrails:special-variable"        foreground="purple_1"/>
  <style name="rubyonrails:symbol"                  foreground="white_1"/>
  <style name="xml:attribute-name"                  foreground="orange_2"/>
  <style name="xml:doctype"                         foreground="orange_2"/>
  <style name="xml:element-name"                    foreground="orange_2"/>
  <style name="xml:namespace"                       foreground="orange_2"/>
  <style name="xml:tag"                             foreground="orange_2"/>
</style-scheme>
